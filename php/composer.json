{
   "name": "litehex/websocket-php",
   "description": "A WebSocket library that helps you to write a server or client without any unnecessary configuration.",
   "keywords": ["php", "library", "websocket", "client",
	  "SocketServer"
   ],
   "authors": [
	  {
		 "name": "Shahrad Elahi",
		 "email": "shahrad@litehex.com"
	  }
   ],
   "require": {
	  "php": ">=8.0",
	  "ext-openssl": "*",
	  "shahradelahi/easy-http": "^v1.2.0"
   },
   "require-dev": {
	  "phpunit/phpunit": "^9.5.20",
	  "vlucas/phpdotenv": "^v5.4.1"
   },
   "provide": {
	  "ext-pcntl": "*",
	  "ext-readline": "*"
   },
   "autoload": {
	  "psr-4": {
		 "Litehex\\WebSocket\\": "src/"
	  }
   },
   "autoload-dev": {
	  "psr-4": {
		 "Litehex\\WebSocket\\Tests\\": "tests/"
	  }
   }
}